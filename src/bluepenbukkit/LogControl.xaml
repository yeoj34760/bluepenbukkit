<UserControl x:Class="bluepenbukkit.LogControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:bluepenbukkit"
             mc:Ignorable="d" 
             d:DesignHeight="442" d:DesignWidth="642" Background="#FF282828">
    <Grid>
        <TextBox x:Name="PrintTextBox" HorizontalAlignment="Left" Height="365" Margin="10,42.5,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="622" Background="#FF323232" Foreground="White" BorderBrush="{x:Null}"/>
        <TextBox x:Name="InputTextBox" HorizontalAlignment="Left" Height="19.5" Margin="10,412.5,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="622" KeyDown="OnKeyDownHandler" BorderBrush="{x:Null}" Background="#FF323232" Foreground="White"/>
        <Button Background="#FF282828" Content="실행" HorizontalAlignment="Left" Height="27.5" Margin="543,10,0,0" VerticalAlignment="Top" Width="89" Click="Button_Click" BorderBrush="{x:Null}" Foreground="White" Style="{DynamicResource ButtonStyle1}"/>

    </Grid>
</UserControl>
