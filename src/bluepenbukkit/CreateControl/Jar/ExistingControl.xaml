<UserControl x:Class="bluepenbukkit.CreateControl.Jar.ExistingControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:bluepenbukkit.CreateControl.Jar"
             mc:Ignorable="d" 
             d:DesignHeight="225" d:DesignWidth="400 " Background="#FF232323">
    <Grid>
        <ScrollViewer HorizontalAlignment="Left" Width="350" FlowDirection="RightToLeft" UseLayoutRounding="False" VerticalScrollBarVisibility="Auto">
            <WrapPanel x:Name="JarListWrapPanel"/>
        </ScrollViewer>
      
        <Button HorizontalAlignment="Right" Width="50" Height="50" BorderBrush="{x:Null}" Background="#FF232323" Style="{DynamicResource ButtonStyle1}" Margin="0" Click="Button_Click">
            <Label FontFamily="MoeumT R" Content="→" Foreground="White" FontSize="36"/>
        </Button>
    </Grid>
</UserControl>
