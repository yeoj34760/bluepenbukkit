<UserControl x:Class="bluepenbukkit.CreateControl.JarChoiceControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:bluepenbukkit.CreateControl"
             mc:Ignorable="d" 
             d:DesignHeight="225" d:DesignWidth="400" Background="#FF232323">
    <Grid>
        <Button x:Name="ExistingFileButton" Content="기존" Width="70" Height="70" Margin="0,0,100,0" Click="ExistingFileButton_Click" BorderBrush="{x:Null}" Background="#FF232323" Foreground="White" Style="{DynamicResource ButtonStyle1}" />
        <Button x:Name="NewFileButton" Content="새 파일" Width="70" Height="70" Margin="100,0,0,0" Click="NewFileButton_Click" Background="#FF232323" BorderBrush="{x:Null}" Foreground="White" Style="{DynamicResource ButtonStyle1}"/>
    </Grid>
</UserControl>
